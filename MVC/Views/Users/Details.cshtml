@model UserModel
@*Generated from Custom Template.*@
@*Model namespace using directive should be added to _ViewImports.cshtml.*@

@{
    ViewData["Title"] = "User Details";
}

<h1>@ViewData["Title"]</h1>

@if (TempData["Message"] is not null)
{
    <p class="bg-info text-black">@TempData["Message"]</p>
}
<div>
    <h4>User</h4>
    <hr />
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.FullNameOutput)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.FullNameOutput)
            </dd>
        </dl>

        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.BirthDateOutput)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.BirthDateOutput)
            </dd>
        </dl>
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.IsAdminOutput)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.IsAdminOutput)
            </dd>
        </dl>
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.EmailOutput)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.EmailOutput)
            </dd>
        </dl>
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.PasswordOutput)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.PasswordOutput)
            </dd>
        </dl>
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.PostsNamesOutput)
            </dt>
            <dd class="col-10">
                @Html.Raw(Model.PostsNamesOutput)
            </dd>
        </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>&nbsp;|&nbsp;
    <a asp-action="Index">Back to List</a>
</div>



